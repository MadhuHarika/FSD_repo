<% if (!!data.customer) { %>
    <% if (!!data.isAccount) { %>
        <h3><small>Account-user</small></h3>
        <div class="vertical-menu">

        
            <a href="/account/blocked">Blocked Accounts</a>
            <a href="/account/update">Account Update</a>
            <a href="/account/search">Search</a>
            <a href="/account/logout">Logout</a>
        </div>


    <% } else {%>
    <h4><small>Upload Media</small></h4>
    <div class="vertical-menu">

        
        <a href="/upload/<%= data.customer._id %>">Single Media</a>
        <a href="/upload/many/<%= data.customer._id %>">Multiple Media</a>
    </div>
<% }}
else {
%>
    <h1><small>Account</small></h1>
    <div class="vertical-menu">
        <a href="/users/login">Login</a>
        <a href="/users/signup">Register</a>
    </div>
<%
}%>
